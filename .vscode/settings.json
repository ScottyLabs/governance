    "[toml]": {
        "editor.defaultFormatter": "tamasfe.even-better-toml"
    },
    "editor.formatOnSave": true,
    "files.insertFinalNewline": true,
    "files.watcherExclude": {
        "Cargo.lock": true,
    },
    "search.exclude": {
        "Cargo.lock": true,
    },
    "files.readonlyInclude": {
        "Cargo.lock": true,
    },
}
