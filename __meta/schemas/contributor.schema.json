{
    "type": "object",
    "required": [
        "full-name",
        "github-username",
        "slack-member-id",
        "email"
    ],
    "properties": {
        "full-name": {
            "type": "string"
        },
        "github-username": {
            "type": "string"
        },
        "slack-member-id": {
            "type": "string",
            "pattern": "^U[A-Z0-9]+$"
        },
        "email": {
            "type": "string",
            "format": "email",
            "pattern": "^[^@]+@andrew\\.cmu\\.edu$"
        }
    },
    "additionalProperties": false
}
